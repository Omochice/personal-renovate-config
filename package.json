{
	"name": "@Omochice/personal-renovate-config",
	"version": "1.11.0",
	"private": true,
	"description": "renovate configuration for me(experimental)",
	"keywords": [
		"renovate"
	],
	"license": "zlib",
	"author": "Omochice",
	"scripts": {
		"fmt": "npm-run-all fmt:package fmt:json",
		"fmt:json": "biome format ./ --write",
		"fmt:package": "sort-package-json",
		"test": "npm-run-all test:*",
		"test:validate": "renovate-config-validator --strict default.json nix.json mise.json playwright.json replacement/*.json grouping/*.json"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"jsonata": "2.1.0",
		"npm-run-all2": "8.0.4",
		"re2": "1.22.3",
		"renovate": "42.52.3",
		"sort-package-json": "3.5.1",
		"vitest": "4.0.15",
		"yaml": "2.8.2"
	},
	"packageManager": "pnpm@10.25.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild",
			"re2"
		],
		"ignoredBuiltDependencies": [
			"better-sqlite3",
			"core-js-pure",
			"dtrace-provider",
			"protobufjs"
		]
	}
}
