{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"description": "Grouping rule for @slidev/",
	"packageRules": [
		{
			"groupName": "slidev monorepo",
			"matchPackageNames": [
				"@slidev/cli",
				"@slidev/client",
				"@slidev/docs",
				"@slidev/parser",
				"@slidev/types"
			]
		}
	]
}
