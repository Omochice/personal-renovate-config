{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"customManagers": [
		{
			"customType": "regex",
			"fileMatch": [
				"^.mise.toml$",
				".mise/config.toml$",
				".config/mise.toml$",
				".config/mise/config.toml$"
			],
			"matchStrings": [
				"nodejs\\s*=\\s*\"v?(?<currentValue>\\S+)\"\\s*#\\s*renovate:\\s*mise",
				"nodejs\\s*=\\s*'v?(?<currentValue>\\S+)'\\s*#\\s*renovate:\\s*mise"
			],
			"datasourceTemplate": "node-version",
			"depNameTemplate": "node"
		}
	]
}
